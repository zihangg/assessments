unt, bytes memory _key) public {\\n        keys[_account] = _key;\\n    }\\n\\n    function getBalance(address _account) public view returns (uint256) {\\n        return balances[_account];\\n    }\\n\\n    function getKeys(address _account) public view returns (bytes memory) {\\n        return keys[_account];\\n    }\\n}\",\"keccak256\":\"0x143dae068066be84ac7348f3be09e84a719149d6d6df884e72c56b75b33eefc9\",\"license\":\"MIT\"}},\"version\":1}",
